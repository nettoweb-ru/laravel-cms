{
  "name": "nettoweb/laravel-cms",
  "description": "Nettoweb content management system for Laravel-based project",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Netto\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "authors": [
    {
      "name": "Alexander Sidorenkov",
      "email": "contact@nettoweb.ru"
    }
  ],
  "require": {
      "php": "^8.2",
      "laravel/framework": "^10.10",
      "laravel/sanctum": "^3.3",
      "laravel/tinker": "^2.8",
      "ext-intl": "*",
      "ext-simplexml": "*",
      "intervention/image": "^3.3.3"
  },
  "require-dev": {
      "barryvdh/laravel-debugbar": "^3.9",
      "fakerphp/faker": "^1.9.1",
      "laravel/breeze": "^1.28",
      "laravel/pint": "^1.0",
      "laravel/sail": "^1.18",
      "mockery/mockery": "^1.4.4",
      "nunomaduro/collision": "^7.0",
      "spatie/laravel-ignition": "^2.0"
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
          "Netto\\CmsServiceProvider"
      ]
    }
  }
}
